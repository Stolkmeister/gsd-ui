{
  "name": "gsd-ui",
  "version": "0.1.0",
  "description": "Dashboard UI for GSD project planning directories",
  "type": "module",
  "bin": {
    "gsd-ui": "cli.ts"
  },
  "files": [
    "cli.ts",
    "server/",
    "dist/"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "server": "bun run server/index.ts",
    "start": "bun run build && bun run server/index.ts",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "gsd",
    "dashboard",
    "project-management",
    "planning"
  ],
  "license": "MIT",
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "chokidar": "^5.0.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.2.0",
    "@types/bun": "latest",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "autoprefixer": "^10.4.24",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.575.0",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "react-router": "^7.13.0",
    "recharts": "^3.7.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.0",
    "vite": "^7.3.1"
  }
}
